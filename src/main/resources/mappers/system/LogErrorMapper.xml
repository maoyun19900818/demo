<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rtmap.core.mapper.LogErrorMapper" >
	
	<select id="select" resultType="com.rtmap.core.domain.LogError">
		select `id`, `api`, `message`, `create_time`
		from system_error_log
		order by id desc 
	</select>
	
	<insert id="insert" parameterType="com.rtmap.core.domain.LogError" >
		insert into system_error_log
	    <trim prefix="(" suffix=")" suffixOverrides="," >
	      <if test="id != null" >
	        id,
	      </if>
	      <if test="api != null" >
	        api,
	      </if>
	      <if test="message != null" >
	        message,
	      </if>
	      <if test="create_time != null" >
	        create_time,
	      </if>
	    </trim>
	    <trim prefix="values (" suffix=")" suffixOverrides="," >
	      <if test="id != null" >
	        #{id},
	      </if>
	      <if test="api != null" >
	        #{api},
	      </if>
	      <if test="message != null" >
	        #{message},
	      </if>
	      <if test="create_time != null" >
	        #{create_time},
	      </if>
	    </trim>
	</insert>
	
</mapper>